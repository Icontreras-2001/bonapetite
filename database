var chicken = ["Chicken Breast, ", "Tablespoon of Paprika, ", "Tablespoon of oil, ", "Mushrooms, ", "Spec of Salt "]
var salad = ["Lettuce, ", "Tomatoes, ", "Olives, ", "Spinach, ", "Olive Oil "]
var chickenLime = ["Chicken Breast, ", "Lemon, ", "Tumeric, ", "Olives, ", "Spec of Pepper "]
var recipe = [chicken, salad, chickenLime]

function dish(ingredient1) {
 var dish1 = "";
 
 switch(ingredient1) {
    case "Chicken Breast":
      dish1 = recipe[0] + recipe[2];
      break;
    case "Paprika":
    case "Mushrooms":
      dish1 = recipe[0];
      break;
    case "Olives":
      dish1 = recipe[1,2];
      break;
    case "Lettuce":
    case "Spinach":
    case "Tomatoes":
      dish1 = recipe[1];
      break;
      }
      return dish1;
      }
      

console.log(dish("Chicken Breast"))






SELECT food.bytes
CREATE "-id_food_name"
